@model IEnumerable<Example.Models.Student> // chú ý chữ @model viết thường

<h1>Thông tin sinh viên lớp k20</h1>
<table class="table">
    <tr>
        <th>STT</th>
        <th>Tên</th>
        <th>Tuổi</th>
        <th>Giới tính</th>
        <th>Hình Ảnh</th>
        <th>Thao tác</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Age</td>
            <td>@(item.Gender ? "Nam" : "Nữ")</td>
            <td><img src="@item.ImgUrl" height="100" /></td>
            <td>
                @Html.ActionLink("Detail", "ListOnlyStudent", new { id = item.Id }) 
                @Html.ActionLink("Edit", "EditStudent", new { id = item.Id }) |
                @Html.ActionLink("Delete", "DelStudent", new { id = item.Id })
            </td>
        </tr>
    }
</table>

<p>@Html.ActionLink("Add", "AddStudent")</p>
